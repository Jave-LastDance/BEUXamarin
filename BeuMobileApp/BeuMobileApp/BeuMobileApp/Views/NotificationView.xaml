<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BeuMobileApp.Views.NotificationView"
             xmlns:vm="clr-namespace:BeuMobileApp.ViewModels"
             Title="Notificaciones"
             >

    <ContentPage.Content>
        <StackLayout VerticalOptions="Center">
            <Button VerticalOptions="Center" Text="Notificaciones" Command="{Binding NotificationCommand}"/>
            <Button VerticalOptions="Center" Text="CJFD" Command="{Binding CJFDCommand}"/>
        </StackLayout>
    </ContentPage.Content>

</ContentPage>
